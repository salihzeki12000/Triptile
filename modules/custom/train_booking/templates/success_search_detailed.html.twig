{#
/**
 * @file success_search_detailed.html.twig
 * Default theme implementation to present Detailed success search data.
 *
 * This template is used when viewing Detailed success search pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_success_search_detailed()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('success_search_detailed') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
